#include<iostream>
using namespace std;
int main()
{
	int n,arr[20],i,j=0,k=0,even[20],odd[20];
	cout<<"Enter array size\n";
	cin>>n;
	cout<<"enter array elements\n";
	for(i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	
	for(i=0;i<n;i++)
	{
		if(arr[i]%2==0)
		{
				even[j]=arr[i];
				j++;
		}
		else
		{
				odd[k]=arr[i];
				k++;
		}
		
	}
	for(i=0;i<j;i++)
	{
		cout<<even[i]<<"\n";
	}
	for(i=0;i<k;i++)
	{
		cout<<odd[i];
	}
}
